<script>
    var a = parseInt(prompt("Nhap diem Hoa:"));
    var b = parseInt(prompt("Nhap diem Ly:"));
    var c = parseInt(prompt("Nhap diem Sinh:"));

    var Avg = (a + b + c ) /3;
    document.write("Diem trung binh "+Avg);

</script>
